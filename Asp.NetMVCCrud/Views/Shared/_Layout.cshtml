<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.6.3.js"></script>
    @RenderSection("styles", false)
</head>
<body>
    <nav class="navbar navbar-light bg-light navbar-expand-md  shadow-sm">
        <div class="container-fluid">
            <span class="navbar-brand">MVC 5</span>
            <div class="navbar-collapse collapse">
                <ul class="navbar-nav nav ms-auto">
                    <li class="nav-item">
                        @Html.ActionLink("Home", "Index", "Home", new { }, new { @class = "nav-link" })
                    </li>
                    <li class="nav-item">
                        @Html.ActionLink("Batches", "Index", "Batches", new { }, new { @class = "nav-link" })
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <section class="container-fluid main">
        @RenderBody()
    </section> 
    @RenderSection("scripts", false)
</body>
</html>